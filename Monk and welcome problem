#include <iostream>

using namespace std;

int main() 
{
    long int n;
    cin>>n;
    int a[n];int b[n];int c[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
     for(int j=0;j<n;j++)
    {
        cin>>b[j];
    }
     for(int k=0;k<n;k++)
    {
        c[k]=a[k]+b[k];
       
    }
    for(int l=0;l<n;l++)
    {
       cout<<c[l]<<" ";
        
    }
   
}
