#include <iostream>

using namespace std;

int main() {
	long m, n,t;
	cin>>n;
    int b[n];
    int c[10001]={0};
	for(long i=0;i<n;i++)
	{
	    cin>>b[i];
	    int k = b[i];
	    c[k]++;
	}
	cin>>t;
    while(t--)
    {
        cin>>m;
        if(c[m]!=0)
        cout<<c[m]<<endl;
        else
        cout<<"NOT PRESENT"<<endl;
    }
}
