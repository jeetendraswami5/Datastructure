#include <iostream>

using namespace std;

int main() {
long n,a[5000],sum=0,j=0;
    cin>>n;
    for(long i=0;i<n;i++)
    { 
        
        cin>>a[j];
        
        if(a[j]==0)
        {while(!a[j])
        {
            j--;
        }
        }
        else 
        {
        j++;
        }
    }
    for(long i=0;i<j;i++) 
    sum+=a[i];
    
    
    cout<<sum;
}
